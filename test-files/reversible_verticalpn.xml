	<arc class="production">
		<source x="0" y="0" ref="node1" />
		<target x="0" y="0" ref="pn1.1" />
	</arc>
	
	<arc class="production">
		<source x="0" y="0" ref="node2" />
		<target x="0" y="0" ref="pn1.2" />			
	</arc>
	
	<arc class="catalysis">
		<source x="0" y="0" ref="node3" />
		<target x="0" y="0" ref="pn1" />			
	</arc>

</sbgn>