<?xml version="1.0" encoding="UTF-8"?>
<sbgn xmlns="http://sbgn.org/libsbgn/0.1">

	<node class="simple chemical" id="node1">
		<label text="Ethanol"/> <!-- fontsize="" etc -->
		<!-- Line breaks are allowed in the text attribute -->
		<bbox x="100" y="100" w="30" h="20"/>
	</node>
	<node class="simple chemical" id="node_ethanal">
		<label text="Ethanal" />
		<bbox x="200" y="100" w="30" h="20"/>
	</node>
	<node class="macromolecule" id="node_adh1">
		<label text="ADH1" />
		<bbox x="150" y="50" w="30" h="20"/>
	</node>
	<node class="simple chemical" id="node_h">
		<label text="H+" />
		<bbox x="200" y="100" w="30" h="20"/>
	</node>
	<node class="simple chemical" id="node_nad">
		<label text="NAD+" />
		<bbox x="200" y="100" w="30" h="20"/>
	</node>
	<node class="simple chemical" id="node_nadh">
		<label text="NADH" />
		<bbox x="200" y="100" w="30" h="20"/>
	</node>
	
	<node class="process" orientation="horizontal" id="pn1">
		<bbox x="150" y="100" w="20" h="10"/>
		<port x="0" y="0" id="pn1.1"/>
		<port x="0" y="0" id="pn1.2"/>
	</node>

	<arc class="consumption">
		<source x="0" y="0" ref="node1" />
		<target x="0" y="0" ref="pn1.1" />
	</arc>
	
	<arc class="consumption">
		<source x="0" y="0" ref="node_nadh" />
		<target x="0" y="0" ref="pn1.1" />			
	</arc>
	
	<arc class="catalysis">
		<source x="0" y="0" ref="node_adh" />
		<target x="0" y="0" ref="pn1" />			
	</arc>

	<arc class="production" >
		<source x="0" y="0" ref="node_h" />
		<target x="0" y="0" ref="pn1.2" />			
	</arc>
	<arc class="production">
		<source x="0" y="0" ref="node_ethanal" />
		<target x="0" y="0" ref="pn1.2" />			
	</arc>
	<arc class="production">
		<source x="0" y="0" ref="node_nad" />
		<target x="0" y="0" ref="pn1.2" />			
	</arc>
</sbgn>